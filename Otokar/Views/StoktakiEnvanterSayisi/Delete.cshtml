@model Otokar.Models.Data.StoktakiEnvanterSayisi

@{
    ViewBag.Title = "Envanter Sil";
}

<h2>Envanter Sil</h2>

<h3>Bu Envanteri Silmek Mi İstiyorsun?</h3>
<div>
    <h4>Stoktaki Envanteri Sil</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Envanter Adı
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EnvanterAdi)
        </dd>

        <dt>
            Envanter Sayısı
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EnvanterSayisi)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Sil" class="btn btn-danger" /> |
            @Html.ActionLink("Listeye Geri Dön", "Index")
        </div>
    }
</div>
